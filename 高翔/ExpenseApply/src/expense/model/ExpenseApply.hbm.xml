	<!-- teacher 教师 -->
	<class name="expense.model.Teacher" table="teacher" catalog="expense_apply">
		<id name="tId" column="TId" type="string" length="10">
			<generator class="assigned"></generator>
		</id>
		<property name="tName" column="TName" type="string" length="20"></property>
		<property name="tUnit" column="TUnit" type="string" length="20"></property>
		<property name="tPosition" column="TPosition" type="string" length="10"></property>
		<property name="tPhone" column="TPhone" type="string" length=""></property>
		<property name="" column="" type="" length=""></property>
		<property name="" column="" type="" length=""></property>
		<property name="" column="" type="" length=""></property>
		<property name="" column="" type="" length=""></property>
	</class>
	
	<!-- technical management area 技术管理领域 -->
	<class name="expense.model.TecMngArea" table="tecmngarea" catalog="expense_apply">
		<id name="tmaCode" column="TMACode" type="string" length="5">
			<generator class="increment"></generator>
		</id>
		<property name="tmaName" column="TMAName" type="string" not-null="true" length="20"></property>
		<property name="tmaInUse" column="TMAInUse" type="boolean" not-null="true"></property>
	</class>
	

	

	<!-- Publisher -->
	<class name="booklib.model.Publisher" table="publisher" catalog="booklib">
		<id name="pubid" column="pubid" type="string" length="20">
			<generator class="assigned"></generator>
		</id>
		<property name="publisherName" column="publisherName" type="string"
			not-null="true" length="50"></property>
		<property name="address" column="address" type="string"
			length="200"></property>
	</class>

	
</hibernate-mapping>
