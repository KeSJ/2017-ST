<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="struts" namespace="/" extends="struts-default">
		<action name="load_blank" class="load_blank">
			<result name="success">blank.jsp</result>
		</action>
	
		<!-- Manage System User -->
		<action name="login" class="login">
			<result name="success">blank.jsp</result>
			<result name="input">login.jsp</result>
		</action>

		<action name="manage_user" class="manage_user">
			<result name="admin">m_user_admin.jsp</result>
			<result name="normal">m_user.jsp</result>
		</action>
		
		<action name="load_add_user" class="load_add_user">
			<result name="success">m_user_add.jsp</result>
		</action>
		
		<action name="add_user" class="add_user">
			<result name="success">m_user_admin.jsp</result>
			<result name="input">m_user_add.jsp</result>
		</action>
		
		<action name="load_modify_user" class="load_modify_user">
			<result name="success">m_user_modify.jsp</result>
		</action>
		
		<action name="modify_user" class="modify_user">
			<result name="admin">m_user_admin.jsp</result>
			<result name="normal">m_user.jsp</result>
			<result name="input">m_user_modify.jsp</result>
		</action>
		
		<action name="load_modify_pwd" class="load_modify_pwd">
			<result name="success">m_pwd_modify.jsp</result>
		</action>
		
		<action name="modify_pwd" class="modify_pwd">
			<result name="success">blank.jsp</result>
			<result name="input">m_pwd_modify.jsp</result>
		</action>
		
		<action name="delete_user" class="delete_user">
			<result name="success">m_user_admin.jsp</result>
		</action>
		
		<!-- End Manage System User -->

		<!-- Manage Reader Type -->		
		<action name="manage_reader_type" class="manage_reader_type">
			<result name="success">m_readertype.jsp</result>
		</action>
		
		<action name="load_add_reader_type" class="load_add_reader_type">
			<result name="success">m_readertype_add.jsp</result>
		</action>
		
		<action name="add_reader_type" class="add_reader_type">
			<result name="success">m_readertype.jsp</result>
			<result name="input">m_readertype_add.jsp</result>
		</action>
		
		<action name="load_modify_reader_type" class="load_modify_reader_type">
			<result name="success">m_readertype_modify.jsp</result>
		</action>
		
		<action name="modify_reader_type" class="modify_reader_type">
			<result name="success">m_readertype.jsp</result>
			<result name="input">m_readertype_modify.jsp</result>
		</action>
		
		<action name="delete_reader_type" class="delete_reader_type">
			<result name="success">m_readertype.jsp</result>
		</action>
		<!-- End Manage Reader Type -->
		
		<!-- Manage Reader -->
		<action name="manage_reader" class="manage_reader">
			<result name="success">m_reader.jsp</result>
		</action>
		
		<action name="load_add_reader" class="load_add_reader">
			<result name="success">m_reader_add.jsp</result>
		</action>
		
		<action name="add_reader" method="execute" class="add_reader">
			<result name="success">m_reader.jsp</result>
			<result name="input">m_reader_add.jsp</result>
		</action>
		
		<action name="load_modify_reader" class="load_modify_reader">
			<result name="success">m_reader_modify.jsp</result>
		</action>
		
		<action name="modify_reader" class="modify_reader">
			<result name="success">m_reader.jsp</result>
			<result name="input">m_reader_modify.jsp</result>
		</action>
		
		<action name="stop_reader" class="stop_reader">
			<result name="success">m_reader.jsp</result>
		</action>
		
		<action name="start_reader" class="start_reader">
			<result name="success">m_reader.jsp</result>
		</action>
		
		<action name="delete_reader" class="delete_reader">
			<result name="success">m_reader.jsp</result>
		</action>
		
		<!-- End Manage Reader -->	
		
		<!-- manage publisher -->
		<action name="manage_publisher" class="manage_publisher">
			<result name="success">m_publisher.jsp</result>
		</action>
		
		<action name="load_add_publisher" class="load_add_publisher">
			<result name="success">m_publisher_add.jsp</result>
		</action>
		
		<action name="add_publisher" class="add_publisher">
			<result name="success">m_publisher.jsp</result>
			<result name="input">m_publisher_add.jsp</result>
		</action>
		
		<action name="load_modify_publisher" class="load_modify_publisher">
			<result name="success">m_publisher_modify.jsp</result>
		</action>
		
		<action name="modify_publisher" class="modify_publisher">
			<result name="success">m_publisher.jsp</result>
			<result name="input">m_publisher_modify.jsp</result>
		</action>
		
		<action name="delete_publisher" class="delete_publisher">
			<result name="success">m_publisher.jsp</result>
		</action>
		
		<!-- end manage publisher -->	
		
		<!-- book -->
		<action name="book" class="book">
			<result name="success">b_book.jsp</result>
		</action>
		
		<action name="load_add_book" class="load_add_book">
			<result name="success">b_book_add.jsp</result>
		</action>
		
		<action name="add_book" class="add_book">
			<result name="success">b_book.jsp</result>
			<result name="input">b_book_add.jsp</result>
		</action>
		
		<action name="load_modify_book" class="load_modify_book">
			<result name="success">b_book_modify.jsp</result>
		</action>
		
		<action name="modify_book" class="modify_book">
			<result name="success">b_book.jsp</result>
			<result name="input">b_book_modify.jsp</result>
		</action>
		
		<action name="stop_book" class="stop_book">
			<result name="success">b_book.jsp</result>
		</action>
		
		<action name="search_barcode" class="search_barcode">
			<result name="success">b_book.jsp</result>
		</action>
		
		<action name="search_bookname" class="search_bookname">
			<result name="success">b_book.jsp</result>
		</action>
		<!-- end book -->
		
		<!-- book lend record -->
		<action name="book_borrow" class="book_borrow">
			<result name="success">b_book_borrow.jsp</result>
		</action>
		
		<action name="borrow" class="borrow">
			<result name="success">b_book_borrow.jsp</result>
		</action>
		
		<action name="book_return" class="book_return">
			<result name="success">b_book_return.jsp</result>
		</action>
		
		<action name="return" class="return">
			<result name="success">b_book_return.jsp</result>
		</action>
		
		<action name="check_book" class="check_book">
			<result name="success">c_book.jsp</result>
		</action>
		
		<action name="check_book_search" class="check_book_search">
			<result name="success">c_book.jsp</result>
		</action>
		
		<action name="check_reader" class="check_reader">
			<result name="success">c_reader.jsp</result>
		</action>
		
		<action name="check_reader_search" class="check_reader_search">
			<result name="success">c_reader.jsp</result>
		</action>
		
		
		<!-- end book lend record -->

	</package>
</struts>  